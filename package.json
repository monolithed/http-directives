{
    "name": "http-directives",
    "description": "A TypeScript-friendly package that provides strongly-typed HTTP constants",
    "keywords": [
        "HTTP Constants",
        "Constants",
        "Response Codes",
        "Status Codes",
        "MIME Types",
        "Cache Control",
        "Charsets",
        "Content Encodings",
        "CSP",
        "DNT",
        "REST API",
        "Headers",
        "Protocols"
    ],
    "version": "1.0.5",
    "license": "MIT",
    "repository": "https://github.com/monolithed/http-directives",
    "type": "module",
    "files": [
        "dist/"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        }
    },
    "scripts": {
        "build": "tsup src --format esm,cjs --dts --out-dir dist --clean",
        "test": "vitest run",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "tsup": "^8.5.0",
        "typescript": "^5.9.3",
        "vitest": "^3.2.4"
    }
}
