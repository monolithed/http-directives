{
    "name": "http-directives",
    "description": "A TypeScript-friendly package that provides strongly-typed HTTP constants",
    "keywords": [
        "HTTP Constants",
        "Constants",
        "Response Codes",
        "MIME Types",
        "Cache Control",
        "Content Encoding",
        "CSP",
        "DNT",
        "Headers"
    ],
    "version": "1.0.2",
    "license": "MIT",
    "repository": "https://github.com/monolithed/http-directives",
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --out-dir dist --clean",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "dist/"
    ],
    "main": "dist/index.js",
    "type": "module",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "devDependencies": {
        "tsup": "8.5.0",
        "typescript": "5.9.3"
    }
}
